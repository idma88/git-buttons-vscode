					"command": "git.stashPopLatest",
					"group": "inline@25"
				},
				{
					"when": "scmProvider == git && config.git-buttons.showGitStashDrop && config.git-buttons.commandsLocation == 'Inline'",
					"command": "git.stashDrop",
					"group": "inline@26"
				},
				{
					"when": "scmProvider == git && config.git-buttons.showGitShowOutput && config.git-buttons.commandsLocation == 'Inline'",
					"command": "git.showOutput",
					"group": "inline@27"
				}
			]
		}
	},
