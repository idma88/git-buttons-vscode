			},
			{
				"category": "Source Control Buttons",
				"command": "git.stashPop",
				"title": "%command.gitStashPop%",
				"icon": "$(repo-pull)"
			},
			{
				"category": "Source Control Buttons",
				"command": "git.stash",
				"title": "%command.gitStash%",
				"icon": "$(repo-push)"
			},
			{
				"category": "Source Control Buttons",
