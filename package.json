			},
			{
				"category": "Source Control Buttons",
				"command": "git.stashApply",
				"title": "%command.gitStashApply%",
				"icon": "$(repo-clone)"
			},
			{
				"category": "Source Control Buttons",
				"command": "git.stashDrop",
