		"menus": {
			"scm/title": [
				{
					"when": "scmProvider == git && config.git-buttons.showGitPull && config.git-buttons.commandsLocation == 'More Actions'",
					"command": "git.pull",
					"group": "navigation@1"
				},
				{
					"when": "scmProvider == git && config.git-buttons.showGitPullRebase && config.git-buttons.commandsLocation == 'More Actions'",
					"command": "git.pullRebase",
					"group": "navigation@2"
				},
				{
					"when": "scmProvider == git && config.git-buttons.showGitPullFrom && config.git-buttons.commandsLocation == 'More Actions'",
					"command": "git.pullFrom",
					"group": "navigation@3"
				},
				{
					"when": "scmProvider == git && config.git-buttons.showGitPush && config.git-buttons.commandsLocation == 'More Actions'",
					"command": "git.push",
					"group": "navigation@4"
				},
				{
					"when": "scmProvider == git && config.git-buttons.showGitPushTo && config.git-buttons.commandsLocation == 'More Actions'",
					"command": "git.pushTo",
					"group": "navigation@5"
				},
